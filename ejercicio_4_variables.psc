Algoritmo ejercicio_4_variables
	definir nombreciudad Como Caracter;
	definir pais Como Caracter;
	
	escribir "¿ingrese el nombre de la capital?"
	leer nombreciudad;
	
	escribir "¿ingrese el nombre del  pais ?"
	leer pais;
	
	escribir " la ciudad de ", nombreciudad, " es la capital del pais  ", pais;
FinAlgoritmo
